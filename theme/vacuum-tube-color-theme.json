{
  "$schema": "vscode://schemas/color-theme",
  "name": "Vacuum tube",
  "type": "dark",
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#db8921"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.regexp", "constant.other.symbol"],
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "name": "Strings: Escape Sequences",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#878787"
      }
    },
    {
      "name": "Numbers, Characters",
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant.keyword",
        "constant"
      ],
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "name": "Global definitions",
      "scope": "entity.name",
      "settings": {
        "foreground": "#DCDCAA"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#878787"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#ffd6d6"
      }
    },
    {
      "name": "Extra: Diff Range",
      "scope": ["meta.diff.range", "meta.diff.index", "meta.separator"],
      "settings": {
        "background": "#DDDDFF",
        "foreground": "#bdbdbd"
      }
    },
    {
      "name": "Extra: Diff From",
      "scope": "meta.diff.header.from-file",
      "settings": {
        "background": "#FFDDDD",
        "foreground": "#bdbdbd"
      }
    },
    {
      "name": "Extra: Diff To",
      "scope": "meta.diff.header.to-file",
      "settings": {
        "background": "#DDFFDD",
        "foreground": "#bdbdbd"
      }
    },
    {
      "name": "Control flow keywords",
      "scope": "keyword.control",
      "settings": {
        "foreground": "#98749d"
      }
    }
  ],
  "colors": {
    "editor.background": "#1E1E1E",
    "editor.foreground": "#D4D4D4",
    "editor.inactiveSelectionBackground": "#3A3D41",
    "editorIndentGuide.background": "#404040",
    "editorIndentGuide.activeBackground": "#707070",
    "editor.selectionHighlightBackground": "#ADD6FF26",
    "list.dropBackground": "#383B3D",
    "activityBarBadge.background": "#db8921",
    "sideBarTitle.foreground": "#BBBBBB",
    "input.placeholderForeground": "#A6A6A6",
    "settings.textInputBackground": "#292929",
    "settings.numberInputBackground": "#292929",
    "menu.background": "#252526",
    "menu.foreground": "#CCCCCC",
    "activityBar.background": "#252526",
    "statusBar.background": "#252526",
    "statusBar.foreground": "#adadad",
    "editorGroup.border": "#1d2225",
    "list.highlightForeground": "#db8921",
    "list.focusBackground": "#373637",
    "list.activeSelectionBackground": "#6f6e6f",
    "pickerGroup.foreground": "#CCCCCC",
    "pickerGroup.border": "#CCCCCC"
  }
}
