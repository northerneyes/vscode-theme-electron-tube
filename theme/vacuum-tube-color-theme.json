{
  "$schema": "vscode://schemas/color-theme",
  "name": "Vacuum tube",
  "type": "dark",
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#db8921"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.regexp", "constant.other.symbol"],
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "name": "Strings: Escape Sequences",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#878787"
      }
    },
    {
      "name": "Global definitions",
      "scope": ["entity.name", "support.function"],
      "settings": {
        "foreground": "#DCDCAA"
      }
    },
    {
      "name": "comparison",
      "scope": ["keyword.operator.comparison", "keyword.operator.relational"],
      "settings": {
        "foreground": "#f1a5ab"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#878787"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "name": "Extra: Diff Range",
      "scope": ["meta.diff.range", "meta.diff.index", "meta.separator"],
      "settings": {
        "background": "#DDDDFF",
        "foreground": "#bdbdbd"
      }
    },
    {
      "name": "Extra: Diff From",
      "scope": "meta.diff.header.from-file",
      "settings": {
        "background": "#FFDDDD",
        "foreground": "#bdbdbd"
      }
    },
    {
      "name": "Extra: Diff To",
      "scope": "meta.diff.header.to-file",
      "settings": {
        "background": "#DDFFDD",
        "foreground": "#bdbdbd"
      }
    },
    {
      "name": "Types declaration and references",
      "scope": [
        "entity.name.type",
        // color of the property name in json/css
        // "support.type.vendored.property-name",
        // "support.type.property-name",
        // and color of other types in type defenition, for example
        "support.type",
        "keyword.control",
        "keyword.operator.typeof",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof",
        "keyword.operator.instanceof",
        "constant.language",
        "storage"
      ],
      "settings": {
        "foreground": "#8c8c8c"
      }
    },
    {
      "name": "Numbers, Characters",
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant.keyword",
        "constant.language",
        "support.constant",
        "constant.other.color",
        "constant.other.symbol",
        "keyword.other.unit",
        "constant"
      ],
      "settings": {
        "foreground": "#d48be0"
      }
    }
  ],
  "colors": {
    "editor.background": "#1E1E1E",
    "editor.foreground": "#D4D4D4",
    "editor.inactiveSelectionBackground": "#3A3D41",
    "editorIndentGuide.background": "#404040",
    "editorIndentGuide.activeBackground": "#707070",
    "editor.selectionHighlightBackground": "#ADD6FF26",
    "list.dropBackground": "#383B3D",
    "activityBarBadge.background": "#db8921",
    "sideBarTitle.foreground": "#BBBBBB",
    "input.placeholderForeground": "#A6A6A6",
    "settings.textInputBackground": "#292929",
    "settings.numberInputBackground": "#292929",
    "menu.background": "#252526",
    "menu.foreground": "#CCCCCC",
    "activityBar.background": "#252526",
    "statusBar.background": "#252526",
    "statusBar.foreground": "#adadad",
    "editorGroup.border": "#1d2225",
    "list.highlightForeground": "#db8921",
    "list.focusBackground": "#373637",
    "list.activeSelectionBackground": "#6f6e6f",
    "pickerGroup.foreground": "#CCCCCC",
    "pickerGroup.border": "#CCCCCC",
    "editorLink.activeForeground": "#db8921",
    "textLink.activeForeground": "#db8921",
    "textLink.foreground": "#db8921"
  }
}
